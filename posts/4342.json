ole": "student", "us": false}, {"admin": false, "endorser": {}, "facebook_id": "100005394402714", "id": "hktwxo5q8h547h", "name": "Max Kanwal", "photo": "1523295706_35.png", "photo_url": "https://cdn-uploads.piazza.com/photos/hktwxo5q8h547h/1523295706_35.png", "published": true, "role": "", "us": false}, {"admin": false, "endorser": {}, "facebook_id": null, "id": "is6p759ji5o5ws", "name": "Vera Wang", "photo": "1529996635_35.png", "photo_url": "https://cdn-uploads.piazza.com/photos/is6p759ji5o5ws/1529996635_35.png", "published": true, "role": "student", "us": false}, {"admin": false, "class_sections": ["classof2019_hyq0br1u3kx7dg"], "endorser": {"jjnjp9mea54uy": 1536696387}, "facebook_id": null, "id": "icguy6ln9pj1pi", "name": "Srinivasa Pranav", "photo": null, "photo_url": null, "published": true, "role": "", "us": false}, {"admin": false, "endorser": {}, "facebook_id": null, "id": "jz9gltnmf1358i", "name": "Andrew Lin", "photo": null, "photo_url": null, "published": true, "role": "student", "us": false}], "tag_endorse_arr": ["idriks20rqg42v", "idrikwpr31m46c", "is6p8idxhub7pd", "hc6qtswtj87bi", "is6p8z72291ay", "ijj65y73ymb5i", "ipf2ezhjrt956a", "ijjujonuuj57nk", "irdxxfmolfj6tn", "hzab6kpr3c048h", "is6p71kv5uj5u0", "hktwxo5q8h547h", "is6p759ji5o5ws", "icguy6ln9pj1pi", "jz9gltnmf1358i"], "type": "i_answer"}], "config": {}, "created": "2017-05-31T20:22:13Z", "data": {"embed_links": []}, "default_anonymity": "no", "drafts": null, "folders": ["cs", "cs-intended"], "history": [{"anon": "stud", "content": "<p>I&#39;m planning on taking CS70 this summer. I&#39;m currently trying to learn as much of the material as possible before the session starts.</p>\n<p></p>\n<p>I can&#39;t find the solutions to some of the discussions and homeworks from CS70 Fall 2016 and Fall 2014. The link seems to be broken for Fall 2016: <a href=\"https://inst.eecs.berkeley.edu/~cs70/fa16/static/discussions/dis01asol.pdf\">https://inst.eecs.berkeley.edu/~cs70/fa16/static/discussions/dis01asol.pdf</a> and I can&#39;t find links to any solutions for Fall 2014.</p>\n<p></p>\n<p>If anyone knows where I can get the solutions or happens to have a copy of the solutions, that would help out a lot. Thanks!</p>", "created": "2017-05-31T20:22:13Z", "subject": "CS70 Previous Semester Discussion Solutions", "uid_a": "a_0"}], "history_size": 1, "i_edits": [], "id": "j3dfubea18tzx", "is_bookmarked": false, "is_tag_good": false, "my_favorite": false, "no_answer": 0, "no_answer_followup": 0, "nr": 4342, "num_favorites": 3, "q_edits": [], "request_instructor": 0, "request_instructor_me": false, "s_edits": [], "status": "active", "t": 1654551990343, "tag_good": [{"admin": false, "endorser": {}, "facebook_id": null, "id": "hzab6kpr3c048h", "name": "Justin Luke", "photo": "1473540382_35.png", "photo_url": "https://cdn-uploads.piazza.com/photos/hzab6kpr3c048h/1473540382_35.png", "published": true, "role": "student", "us": false}], "tag_good_arr": ["hzab6kpr3c048h"], "tags": ["cs", "cs-intended", "student"], "type": "question", "unique_views": 495}