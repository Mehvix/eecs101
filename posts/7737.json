, {"admin": false, "endorser": {}, "facebook_id": null, "id": "is6p6nezwwv5jd", "name": "Aryana Shabestari", "photo": null, "photo_url": null, "published": true, "role": "student", "us": false}], "tag_good_arr": ["hx22q5r8G96", "iduvej8fsth3dj", "j6d4w0h8ra37l9", "is6p6j9n6u55fp", "iy0vm41jwf95jb", "is6p7gejsqw65o", "hzd1lb5fiuc2sq", "j6e9lx8x7gb1uy", "ipkfr8wxmo61w9", "is6p6nezwwv5jd"], "tags": ["eecs", "lscs", "peer_directed", "student"], "type": "question", "unique_views": 418}