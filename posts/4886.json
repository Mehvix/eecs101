ldren": [{"anon": "stud", "bucket_name": "Week 8/27 - 9/2", "bucket_order": 135, "children": [], "config": {}, "created": "2017-08-28T19:58:31Z", "data": {"embed_links": null}, "folders": [], "id": "j6wl6mzbeyj3zr", "subject": "<p>Thanks, I really appreciate both responses.\u00a0</p>", "tag_good": [], "tag_good_arr": [], "type": "feedback", "uid_a": "a_0", "updated": "2017-08-28T19:58:31Z"}], "config": {}, "created": "2017-08-28T18:16:45Z", "data": {"embed_links": null}, "folders": [], "id": "j6whjrh4anu7ne", "no_answer": 0, "no_upvotes": 0, "subject": "<p>Professor Sahai has a great response! To add, Professor Joe Hellerstein has a nice list of tutorials on how to read a paper - you can find them at the top of the page here:\u00a0<a href=\"https://sites.google.com/site/cs286fall2014/home/reading-list\">https://sites.google.com/site/cs286fall2014/home/reading-list</a>\u00a0</p>", "tag_good": [], "tag_good_arr": [], "type": "followup", "uid_a": "a_1", "updated": "2017-08-28T18:16:45Z"}], "config": {}, "created": "2017-08-27T05:54:31Z", "data": {"embed_links": []}, "default_anonymity": "no", "drafts": null, "folders": ["other"], "history": [{"anon": "stud", "content": "<p>Hi all,</p>\n<p></p>\n<p>I&#39;ve been assigned to read some papers from a potential research mentor and its been a very discouraging process so far. All of the papers reference so many things that I haven&#39;t seen before and I&#39;m finding myself going down a rabbit hole of wikipedia articles, trying to understand what I&#39;m reading. For reference, the papers I&#39;ve been assigned are on ml/reinforcement learning, and\u00a0the upperdivs I&#39;ve taken are 170, 110, 188, and 126 (I&#39;m taking 189 this semester).\u00a0</p>\n<p></p>\n<p>Anyway I guess I&#39;m looking at general advice on how to read research papers, and\u00a0wondering how much struggle is normal vs how much is due to an\u00a0insufficient background).\u00a0</p>", "created": "2017-08-27T05:54:31Z", "subject": "How to read research papers", "uid_a": "a_0"}], "i_edits": [], "id": "j6ublejic4t62k", "is_bookmarked": false, "is_tag_good": false, "my_favorite": false, "no_answer": 0, "no_answer_followup": 0, "nr": 4886, "num_favorites": 17, "q_edits": [], "request_instructor": 0, "request_instructor_me": false, "s_edits": [], "status": "active", "t": 1654551526058, "tag_good": [{"admin": false, "endorser": {}, "facebook_id": null, "id": "idriks20rqg42v", "name": "Hrishi Dharam", "photo": "1544372913_35.png", "photo_url": "https://d1b10bmlvqabco.cloudfront.net/photos/idriks20rqg42v/1544372913_35.png", "published": true, "role": "student", "us": false}, {"admin": false, "endorser": {}, "facebook_id": null, "id": "idkkuk3zhbm6aw", "name": "Hanyu Li", "photo": null, "photo_url": null, "published": true, "role": "student", "us": false}, {"admin": false, "class_sections": ["classof2019_hyq0br1u3kx7dg"], "endorser": {}, "facebook_id": null, "id": "icguxwqmsgc1ic", "name": "Sirej Dua", "photo": null, "photo_url": null, "published": true, "role": "student", "us": false}, {"admin": false, "endorser": {}, "facebook_id": null, "id": "ijj65y73ymb5i", "name": "Amy Mendelsohn", "photo": null, "photo_url": null, "published": true, "role": "student", "us": false}, {"admin": false, "endorser": {}, "facebook_id": null, "id": "is6p7gejsqw65o", "name": "Gokul Swamy", "photo": "1553076556_35.png", "photo_url": "https://d1b10bmlvqabco.cloudfront.net/photos/is6p7gejsqw65o/1553076556_35.png", "published": true, "role": "student", "us": false}], "tag_good_arr": ["idriks20rqg42v", "idkkuk3zhbm6aw", "icguxwqmsgc1ic", "ijj65y73ymb5i", "is6p7gejsqw65o"], "tags": ["other", "student"], "type": "question", "unique_views": 644}